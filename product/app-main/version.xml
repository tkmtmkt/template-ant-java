<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

    <info organisation="example.com"
          module="app-main"
          revision="0.1"
          status="integration"/>

    <configurations defaultconfmapping="compile->default;test->default">
        <conf name="default" extends="runtime,master" />
        <conf name="master" />
        <conf name="compile" />
        <conf name="runtime" extends="compile" />
        <conf name="test" visibility="private" extends="runtime" />
        <conf name="sources" />
        <conf name="javadoc" />
    </configurations>

    <publications>
        <artifact name="app-main" type="jar" ext="jar" conf="master" />
        <artifact name="app-main" type="src" ext="jar" conf="sources" />
        <artifact name="app-main" type="doc" ext="jar" conf="javadoc" />
    </publications>

    <dependencies>
        <dependency name="app-data" rev="latest.integration" conf="compile" />
        <dependency org="org.slf4j" name="slf4j-api" rev="1.7.21" conf="compile" />
        <dependency org="junit" name="junit" rev="4.12" conf="test" />
        <dependency org="org.spockframework" name="spock-core" rev="1.0-groovy-2.4" conf="test" />
    </dependencies>
</ivy-module>
<!-- vim: set ts=4 sw=4 et: -->
